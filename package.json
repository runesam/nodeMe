{
	"name": "node-me",
	"version": "1.0.0",
	"description": "me exploring node",
	"main": "index.js",
	"scripts": {
		"weather": "nodemon index --app weather",
		"notes": "nodemon index --app notes",
		"server": "nodemon index --app server -e js,hbs",
		"todo": "nodemon index --app todo -e js,hbs",
		"start": "node index --app todo",
		"testServer": "mocha ./serverApp -name '*test.js' --require ignore-styles  --require babel-register --recursive",
		"testTodo": "mocha ./todoApp -name '*test.js' --require ignore-styles  --require babel-register --recursive --watch",
		"test:server": "yarn server --exec \"yarn testServer\"",
		"test:todo": "yarn todo --exec \"yarn testTodo\""
	},
	"repository": "git@github.com:runesam/nodeMe.git",
	"author": "runesam <same7.hamada@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.18.0",
		"babel-core": "^6.26.3",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-register": "^6.26.0",
		"body-parser": "^1.18.3",
		"express": "^4.16.4",
		"hbs": "^4.0.1",
		"moment": "^2.22.2",
		"mongodb": "^3.1.10",
		"mongoose": "^5.3.13",
		"yargs": "^12.0.2"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"eslint": "^5.3.0",
		"eslint-config-airbnb": "17.1.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.11.0",
		"ignore-styles": "^5.0.1",
		"mocha": "^5.2.0",
		"supertest": "^3.3.0"
	}
}
